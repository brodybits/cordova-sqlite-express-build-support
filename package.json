{
  "name": "cordova-sqlite-express-build-support",
  "version": "0.1.0-dev",
  "description": "Native interface to SQLite for PhoneGap/Cordova - cordova-sqlite-express-build-support plugin version",
  "cordova": {
    "id": "cordova-sqlite-express-build-support",
    "platforms": [
      "android",
      "ios",
      "osx"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brodybits/cordova-sqlite-express-build-support.git"
  },
  "keywords": [
    "sqlite",
    "ecosystem:cordova",
    "cordova-android",
    "cordova-ios",
    "cordova-osx"
  ],
  "author": "various",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brodybits/cordova-sqlite-express-build-support/issues"
  },
  "homepage": "https://github.com/brodybits/cordova-sqlite-express-build-support",
  "scripts": {
    "clean-spec": "rm -rf spec/[mnp]* && git cl spec/config.xml && git status --ignored",
    "prepare-js": "coffee -p SQLitePlugin.coffee.md > www/SQLitePlugin.js",
    "prepare-spec": "node scripts/prepareSpec.js"
  },
  "devDependencies": {
    "coffeescript": "1",
    "cross-spawn": "^6.0.5",
    "fs-extra": "^8.1.0"
  }
}
